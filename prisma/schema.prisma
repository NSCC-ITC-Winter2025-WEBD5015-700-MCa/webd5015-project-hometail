generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name String?
  email String? @unique 
  password String?
  isSubscribed Boolean @default(false)
  customerId String? @unique
  priceId String?
}

model Dog {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  breed String?
  name String?
  size String?
  activityLevel String?
  shedding String?
  location String?
 
}





  
    
   
   


