generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name String?
  email String? @unique 
  password String?
  isSubscribed Boolean @default(false)
  customerId String? @unique
  priceId String?
}

model DogBreed {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  breed String?
}

model Dog {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  name           String
  breed          String
  size           String
  activityLevel  String
  goodWithKids   Boolean
  temperament    String
  shedding       String
  maintenanceCost String
  location       String
  image          String?
  createdAt      DateTime @default(now())
}






  
    
   
   


